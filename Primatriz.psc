Algoritmo Primatriz
	definir matriz Como Entero;
	Dimensionar matriz[4,5];
	definir i,j Como Entero;
	definir contador como entero;
	contador=1;
	para i=1 hasta 4 Hacer
		para j=1 hasta 5 Hacer
			matriz[i,j]= contador;
			contador=contador+1;
		FinPara
	FinPara
	
	para i=1 hasta 4 Hacer
		para j=1 hasta 5 Hacer
			escribir  Sin Saltar matriz[i,j],"  ";
		FinPara
	Escribir "";
	FinPara
FinAlgoritmo

	
	
	
	
	
	
	
	

