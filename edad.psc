Algoritmo num
	definir contador Como Entero;
	definir numero como entero;
	Escribir "introduce un numero entre 2 y 10 ";
	leer numero;
	Si (numero<2) y (numero>10)Entonces
		Escribir "error";
	SiNo
		contador=1;
		Mientras (contador<=numero) Hacer
			Escribir Sin Saltar "*";
			contador= contador+1;
			
			
			
			
	fin mientras
	Fin Si
	
	
	
FinAlgoritmo
