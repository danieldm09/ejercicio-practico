Algoritmo Factorial
	definir fact Como Entero;
	definir numero como entero;
	definir contador como entero;
	contador=1;
	escribir"introduce un numero";
	Leer numero;
	fact=1;
	mientras contador<=numero Hacer
		fact= fact*contador;
		contador=contador+1;

	FinMientras
	escribir fact;
FinAlgoritmo

	
	
	
	
	
	
	
	

